#, scheme-format
msgid "~A: package not found for version ~a~%"
msgstr "~A : paquet introuvable pour la version ~a~%"

#: gnu/packages.scm:284
#, scheme-format
msgid "~A: unknown package~%"
msgstr "~A : paquet inconnu~%"

#: gnu/packages.scm:312
#, scheme-format
