#: doc/guix.texi:9970
#, no-wrap
msgid "--sort=@var{key}"
msgstr ""

#. type: table
#: doc/guix.texi:9972
msgid "Sort lines according to @var{key}, one of the following options:"
msgstr ""

#. type: item
#: doc/guix.texi:9974
#, no-wrap
msgid "self"
msgstr ""

#. type: table
#: doc/guix.texi:9976
msgid "the size of each item (the default);"
msgstr ""

#. type: table
