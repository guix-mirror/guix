msgstr ""

#. type: deftypevr
#: doc/guix.texi:15069
msgid "Defaults to @samp{actions}."
msgstr ""

#. type: deftypevr
#: doc/guix.texi:15071
#, no-wrap
msgid "{@code{cups-configuration} parameter} boolean auto-purge-jobs?"
msgstr ""

#. type: deftypevr
#: doc/guix.texi:15074
